{
    "name": "cjeruen/view-for-zanphp",
    "description": "FastRoute for zanphp",
    "keywords": ["twig", "blade", "zanphp"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cjeruen/view-for-zanphp/issues",
        "source": "https://github.com/laravel/view-for-zanphp"
    },
    "authors": [
        {
            "name": "Hui Chen",
            "email": "cjeruen@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "zanphp/zan": "dev-master",
        "illuminate/view": "^5.6@dev",
        "twig/twig": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Com\\JeRuen\\Zan\\View\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "prefer-stable": false,
    "minimum-stability": "dev"
}
